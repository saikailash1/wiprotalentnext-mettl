		// Write code here...
		 int i,m=0,flag=0;      
  int n=input1;//it is the number to be checked    
  m=n/2;      
  if(n==0||n==1)
  {  
   return 1;   
  }
  else{  
   for(i=2;i<=m;i++){      
    if(n%i==0){      
     flag=1;      
     break;      
    }      
   }      
   if(flag==0)
   {
	   return 2;
   }
   else
   {
	   return 1;
   }
  }//end of else  
		
	}
}
